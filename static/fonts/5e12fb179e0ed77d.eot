    function myPagination(_ref) {
        var pageSize = _ref.pageSize,
            pageTotal = _ref.pageTotal,
            curPage = _ref.curPage,
            id = _ref.id,
            getPage = _ref.getPage,
            showPageTotalFlag = _ref.showPageTotalFlag,
